{
  "name": "overseer-ui",
  "version": "2.0.0",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "electron-build": "ng build --base-href ./ && electron .",
    "electron-build-prod": "ng build --prod --aot --base-href ./ && electron .",
    "electron-pack-win": "ng build --prod --aot --base-href . && electron-builder -w",
    "electron-publish-win": "ng build --prod --aot --base-href . && electron-builder -w -p always"
  },
  "build": {
    "appId": "com.overseer.app",
    "productName": "Overseer",
    "directories": {
      "output": "packaged"
    },
    "publish": {
      "provider": "spaces",
      "name": "velavanpharmacy",
      "region": "sgp1",
      "path": "overseer-releases"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.2",
    "@angular/cdk": "^6.0.2",
    "@angular/common": "^6.0.2",
    "@angular/compiler": "^6.0.2",
    "@angular/core": "^6.0.2",
    "@angular/flex-layout": "6.0.0-beta.15",
    "@angular/forms": "^6.0.2",
    "@angular/http": "^6.0.2",
    "@angular/material": "^6.0.2",
    "@angular/platform-browser": "^6.0.2",
    "@angular/platform-browser-dynamic": "^6.0.2",
    "@angular/router": "^6.0.2",
    "bootstrap": "^4.1.2",
    "core-js": "^2.5.4",
    "electron-is-dev": "^0.3.0",
    "electron-store": "^2.0.0",
    "electron-updater": "^2.23.3",
    "lodash": "^4.17.5",
    "ngx-electron": "^1.0.4",
    "rxjs": "^6.0.0",
    "systeminformation": "^3.40.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.3",
    "@angular-devkit/build-ng-packagr": "~0.6.3",
    "@angular/cli": "~6.0.3",
    "@angular/compiler-cli": "^6.0.2",
    "@angular/language-service": "^6.0.2",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "electron": "^2.0.3",
    "electron-builder": "^20.18.0",
    "electron-publisher-s3": "^20.17.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0-rc.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
